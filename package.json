{
  "name": "genetic-mutation",
  "version": "0.0.0",
  "description": "Genetic Differences Detection Project in DNA Sequence.",
  "repository": "https://gitlab.com/jesuslares/genetic-mutation",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./src/index.ts",
    "dev:windows": "set NODE_ENV=development && ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./src/index.ts",
    "prepare": "husky install && npx ts-patch install -s",
    "lint": "eslint './src/**/*.ts'",
    "lint:fix": "eslint './src/**/*.ts' --fix",
    "clean": "rm -rf coverage src/**/*.js src/**/*.map",
    "start": "node build/src/index.js",
    "build": "npm run clean -rf ./build && tsc --project ./",
    "test": "NODE_ENV=test && export NODE_OPTIONS='--max-old-space-size=4096' && jest --silent --detectOpenHandles --runInBand --no-cache",
    "test:windows": "set NODE_ENV=test && set NODE_OPTIONS=--max-old-space-size=4096 && jest --silent --detectOpenHandles --runInBand --no-cache",
    "test:server": "NODE_ENV=test && export NODE_OPTIONS='--max-old-space-size=4096' && jest --no-cache --runInBand --detectOpenHandles",
    "test:server:windows": "set NODE_ENV=test && jest --no-cache --runInBand --detectOpenHandles",
    "test:watch": "npm run test:server -- --verbose --watch ",
    "test:watch:windows": "npm run test:server:windows -- --verbose --watch "
  },
  "keywords": [],
  "author": "Jesus Lares Cotreras",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "8.2.0",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.3",
    "@types/morgan": "^1.9.4",
    "@types/node": "^17.0.33",
    "@types/supertest": "2.0.12",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "eslint": "8.47.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "husky": "8.0.3",
    "jest": "^29.6.3",
    "prettier": "3.0.2",
    "supertest": "6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "ts-patch": "3.0.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.6.4",
    "typescript-transform-paths": "3.4.6"
  },
  "dependencies": {
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "helmet": "7.0.0",
    "joi": "17.9.2",
    "morgan": "1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.0"
  }
}
