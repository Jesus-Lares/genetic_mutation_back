{
  "name": "genetic-mutation",
  "version": "0.0.0",
  "description": "Genetic Differences Detection Project in DNA Sequence.",
  "repository": "https://gitlab.com/jesuslares/genetic-mutation",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./src/index.ts",
    "dev:windows": "set NODE_ENV=development && ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./src/index.ts",
    "prepare": "husky install && npx ts-patch install -s",
    "lint": "eslint './src/**/*.ts'",
    "lint:fix": "eslint './src/**/*.ts' --fix",
    "clean": "rm -rf coverage src/**/*.js src/**/*.map",
    "build": "npm run clean -rf ./build && tsc --project ./"
  },
  "keywords": [],
  "author": "Jesus Lares Cotreras",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "eslint": "8.47.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "husky": "8.0.3",
    "prettier": "3.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "ts-patch": "3.0.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.6.4",
    "typescript-transform-paths": "3.4.6"
  }
}
